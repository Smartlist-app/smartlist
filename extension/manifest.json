{
  "name": "Smartlist Launcher",
  "description": "The Sophisticated home inventory app",
  "version": "1.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": ["contextMenus", "storage"],
  "background": {
    "scripts": [ "locales.js", "background.js"],
    "persistent": true },
  "browser_action": {
    "default_popup": "options.html"
  },
  "icons": {
   "16": 
"globalGoogle16.png",
   "48": "globalGoogle48.png",
  "128": "globalGoogle128.png"
 }
}
