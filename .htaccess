RewriteEngine On
# Redirect to index file
RewriteCond %{THE_REQUEST} ^.*/index\.php 
RewriteRule ^(.*)index.php$ /$1 [R=301,L]
# FRIENDLY URLS -------------------------------------------------------------
RewriteRule ^login(.*)$ dashboard/login.php?id=$1 [L]
RewriteRule ^signup(.*)$ dashboard/register.php?id=$1 [L]
RewriteRule ^dashboard/beta(.*)$ dashboard/test.php?id=$1 [L]
# ERROR REDIRECTS------------------------------------------------------------
ErrorDocument 400 https://smartlist.ga/error/400
ErrorDocument 401 https://smartlist.ga/error/403
ErrorDocument 403 https://smartlist.ga/error/403
ErrorDocument 404 https://smartlist.ga/error/404
ErrorDocument 503 https://smartlist.ga/error/500
# SPEED----------------------------------------------------------------------
Header set Connection keep-alive
order deny,allow
Header set Connection keep-alive
AddType text/cache-manifest .manifest
<filesMatch ".(css|jpg|jpeg|png|gif|js|ico)$">
    Header set Cache-Control "max-age=2592000, public"
</filesMatch>
